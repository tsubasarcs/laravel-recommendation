{
  "name": "tsubasarcs/laravel-recommendation",
  "description": "cp member get member database package",
  "license": "MIT",
  "homepage": "https://github.com/tsubasarcs/laravel-recommendation",
  "authors": [
    {
      "name": "tsubasarcs",
      "email": "tsubasarcs@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "^5.5",
    "illuminate/database": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "^3.5",
    "orchestra/database": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Tsubasarcs\\Recommendations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tsubasarcs\\Recommendations\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "component": "package",
      "providers": [
        "Tsubasarcs\\Recommendations\\RecommendationServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
